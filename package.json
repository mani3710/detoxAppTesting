{
  "name": "detoxapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "detox:build:ios-debug": "detox build --configuration ios.sim.debug",
    "detox:e2e:ios-debug": "DETOX_CONFIGURATION=ios.sim.debug npx cucumber-js",
    "detox:build:ios-release": "detox build --configuration ios.sim.release",
    "detox:e2e:ios-release": "DETOX_CONFIGURATION=ios.sim.release npx cucumber-js",
    "detox:build:android-debug": "detox build --configuration android.emu.debug",
    "detox:e2e:android-debug": "DETOX_CONFIGURATION=android.emu.debug npx cucumber-js",
    "detox:build:android-release": "detox build --configuration android.emu.release",
    "detox:e2e:android-release": "DETOX_CONFIGURATION=android.emu.release npx cucumber-js"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.14",
    "@react-navigation/material-top-tabs": "^6.6.8",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/stack": "^6.3.21",
    "install": "^0.13.0",
    "npm": "^10.4.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-asset": "^2.1.1",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-pager-view": "^6.2.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-vector-icons": "^10.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.20.0",
    "@cucumber/cucumber": "^10.3.1",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "detox": "^20.19.5",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
